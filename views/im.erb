<div class="row" id="app6">
    <button type="button" onClick="idk()" class="btn btn-dark btn-lg btn-block">Submit</button>
    <input v-model="message" style="width: 100%;margin:10px;">
    <script>
var app6 = new Vue({
    el: '#app6',
    data: {
        message: "test1"
    }
})
var client;
$(function() {
    client = new Faye.Client('/faye');
});
function idk() {
    var publication = client.publish('/result/red', {text: app6.message });
    publication.then(function() {
        alert('Message received by server!');
    }, function(error) {
        alert('There was a problem: ' + error.message);
    });
    submit();
}
    </script>
